<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <system.serviceModel>
    <client>
      <endpoint address="net.tcp://localhost:8009/LibraryManager"
                binding="netTcpBinding"
                contract="OneComic.Client.Contracts.ILibraryService" />
      <endpoint address="net.tcp://localhost:8009/BookmarkManager"
                binding="netTcpBinding"
                contract="OneComic.Client.Contracts.IBookmarkService" />
      <endpoint address="net.tcp://localhost:8009/AccountManager"
                binding="netTcpBinding"
                contract="OneComic.Client.Contracts.IAccountService" />
    </client>
    <bindings>
      <netTcpBinding>
        <binding maxReceivedMessageSize="2000000" transactionFlow="true">
          <reliableSession enabled="true"/>
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>

</configuration>