<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
  <connectionStrings>
    <add name="OneComic"
         connectionString="Data Source=.;Initial Catalog=CarRental;Integrated Security=True"
         providerName="System.Data.SqlClient" />
  </connectionStrings>

  <system.serviceModel>
    <services>
      <service name="OneComic.Business.Managers.Managers.LibraryManager">
        <endpoint address="net.tcp://localhost:8009/LibraryManager"
                  binding="netTcpBinding"
                  contract="OneComic.Business.Contracts.ILibraryService" />
      </service>
      <service name="OneComic.Business.Managers.Managers.BookmarkManager">
        <endpoint address="net.tcp://localhost:8009/BookmarkManager"
                  binding="netTcpBinding"
                  contract="OneComic.Business.Contracts.IBookmarkService" />
      </service>
      <service name="OneComic.Business.Managers.Managers.AccountManager">
        <endpoint address="net.tcp://localhost:8009/AccountManager"
                  binding="netTcpBinding"
                  contract="OneComic.Business.Contracts.IAccountService" />
      </service>
    </services>
    <bindings>
      <netTcpBinding>
        <binding maxReceivedMessageSize="2000000" transactionFlow="true">
          <reliableSession enabled="true"/>
        </binding>
      </netTcpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  
  </startup>
</configuration>